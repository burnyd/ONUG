Read only with gnmi client
env /vagrant/scripts/configs/env.sh
gnmi  -addr leaf2a:6030 -username vagrant -password vagrant subscribe /network-instances/network-instance[name=default]/protocols/protocol[identifier=BGP][name=BGP]/bgp/
gnmi  -addr leaf2a:6030 -username vagrant -password vagrant replace '/network-instances/network-instance[name=default]/protocols/protocol[name=BGP][identifier=BGP]/bgp/global' '{"config":{"as": 1234, "router-id": "1.2.3.4"}}'
gnmi  -addr leaf2a:6030 -username vagrant -password vagrant update '/network-instances/network-instance[name=default]/protocols/protocol[name=BGP][identifier=BGP]/bgp/global' '{"config":{"as": 1234, "router-id": "1.2.3.4"}}'


